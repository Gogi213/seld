{
  "name": "seld",
  "private": true,
  "version": "0.0.0",
  "main": "main.cjs",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "concurrently \"npm:dev\" \"npm:wait-electron\" \"npm:backend\"",
    "wait-electron": "wait-on http://localhost:5173 && electron .",
    "electron": "electron .",
    "backend": "node --max-old-space-size=1024 --expose-gc backend-refactored.cjs",
    "backend-old": "node backend.cjs",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "lightweight-charts": "^4.2.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "concurrently": "^9.2.0",
    "electron": "^37.2.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "vite": "^7.0.0",
    "wait-on": "^8.0.3"
  }
}
